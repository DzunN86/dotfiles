# Git
alias gc="git commit -m"
alias gca="git commit -a -m"
alias gp="git push origin HEAD"
alias gpu="git pull origin"
alias gst="git status"
alias glog="git log --graph --topo-order --pretty='%w(100,0,6)%C(yellow)%h%C(bold)%C(black)%d %C(cyan)%ar %C(green)%an%n%C(bold)%C(white)%s %N' --abbrev-commit"
alias gdiff="git diff"
alias gco="git checkout"
alias gb='git branch'
alias gba='git branch -a'
alias gadd='git add'
alias ga='git add -p'
alias gcoall='git checkout -- .'
alias gr='git remote'
alias gre='git reset'

# ZSH
alias zshco="nvim ~/.zshrc"
alias ohmyzsh="nvim ~/.oh-my-zsh"
alias szsh="source ~/.zshrc"

# Dirs
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ......="cd ../../../../.."

# Larevel
alias artisan="php artisan"
alias lrs="php artisan serve"
alias lmfs="php artisan migrate:fresh --seed"
alias lkg="php artisan key:generate"
alias lro="php artisan optimize"
alias lsl="php artisan storage:link"

# Eza (better ls)
alias ls="eza -l --icons --git -a"
alias lt="eza --tree --level=2 --long --icons --git"
alias ltree="eza --tree --level=2  --icons --git"

# Others
alias cl='clear'
alias cat="bat"
alias cd="z"
alias kittyco="nvim ~/.config/kitty/kitty.conf"
alias nr="npm run"
alias ni="npm install"
alias v="nvim"
alias vco="nvim ~/.config/nvim"


# Bindings
bindkey -s ^f "tmux-sessionizer\n"
